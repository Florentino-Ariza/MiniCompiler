Program{
	func_Decl{
		Type:Int
		Name:xx
		var_decl_Stat{
			var_Decl{
				Type:Int
				Name:x
				mul_Expr{
					add_Expr{
						mul_Expr{
							var_Expr{Name:a}
							var_Expr{Name:b}
						}
						var_Expr{Name:c}
					}
					var_Expr{Name:d}
				}
			}
		}
		return_Stat{
		var_Expr{Name:x}
		}
	}
	func_Decl{
		Type:Void
		Name:print
	}
	func_Decl{
		Type:Int
		Name:main
		var_decl_Stat{
			var_Decl{
				Type:Int
				Name:aa
				val_Expr{
					int:1
				}
			}
		}
		var_decl_Stat{
			var_Decl{
				Type:Double
				Name:b
			}
		}
		for_Stat{
			var_Decl{
				Type:Int
				Name:aa
				val_Expr{
					int:0
				}
			}
			lt_Expr{
				var_Expr{Name:aa}
				val_Expr{
					int:3
				}
			}
			dadd_Expr{
				var_Expr{Name:aa}
			}
			stats_Stat{
				break_Stat
			}
		}
		while_Stat{
			lt_Expr{
				var_Expr{Name:aa}
				val_Expr{
					int:1
				}
			}	
			stats_Stat{
				expr_Stat{
					dadd_Expr{
						var_Expr{Name:aa}
					}
				}
				break_Stat
			}
		}
		return_Stat{
			val_Expr{
				int:0
			}
		}
	}
}
